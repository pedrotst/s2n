decode_ov <- admit "decode"
    [ get_threshold_ov
    , compute_counter_of_unsat_ov
    , compute_syndrome_ov
    , fix_black_error_ov
    , fix_error1_ov
    , fix_gray_error_ov
    , count_ones_R_BITS_ov8
    , count_ones_R_BITS_ov
    , count_ones_N_SIZE_ov
    , count_ones_10_ov
    , cyclic_product_ov ]
